{"components/products/options/set-select":"<div class=\"form-field product-option-popup\" data-product-attribute=\"set-select\">\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_{{id}}\">\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n        {{ this.display_name }}:\r\n        {{#if display_name '==' \"Choose Frequency\"}}\r\n            <a class=\"details-link\" href=\"#\">Learn More</a>\r\n        {{/if}}\r\n    </label>\r\n\r\n    <select class=\"form-select form-select--small\" style=\"padding-right: 4rem;\" name=\"attribute[{{this.id}}]\" id=\"attribute_{{id}}\" {{#if required}}required{{/if}}>\r\n        <option value=\"\">{{lang 'products.choose_options'}}</option>\r\n        {{#each this.values}}\r\n            {{#if ../display_name '==' \"Choose Frequency\"}} \r\n                {{#if label '==' 'Send every 3 months'}}\r\n                {{else if label '==' 'Send every 4 months'}}\r\n                {{else if label '==' 'Send every 5 months'}}\r\n                {{else if label '==' 'Send every 6 months'}}\r\n                {{else}}\r\n                    <option data-product-attribute-value=\"{{id}}\" value=\"{{id}}\" {{#if selected}}selected{{/if}} {{#if label '!=' 'One-time Purchase'}}class = \"recurring\" {{/if}}>{{label}}</option>\r\n                {{/if}}\r\n\r\n            {{else}}\r\n                <option data-product-attribute-value=\"{{id}}\" value=\"{{id}}\" {{#if selected}}selected{{/if}}>{{label}}</option>\r\n            {{/if}}\r\n        {{/each}}\r\n    </select>\r\n    <div class=\"mfp-hide\" id=\"autoship-popup\">\r\n        <div class=\"img-container\">\r\n            <img class=\"popup-img\" src=\"/product_images/uploaded_images/autoship-logo-2020.png\" alt=\"\" >\r\n        </div>\r\n        <h2>Zogics Autoship Program</h2>\r\n        <div class=\"message-left message-divs\">\r\n            <p class=\"message-title\">Save time & schedule your orders</p>\r\n            <p class=\"message\">Choose your delivery frequency to meet your needs, from 1 week to 3 months, on eligible products. Never run out of refills again!</p>\r\n        </div>\r\n        <div class=\"message-right message-divs\">\r\n            <p class=\"message-title\">Manage your autoshipments</p>\r\n            <p class=\"message\">Update your payment method, skip a delivery, cancel your autoshipment, & more right from your account.</p>\r\n        </div>\r\n\r\n    </div>\r\n</div>\r\n"}
