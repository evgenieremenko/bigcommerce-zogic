{"components/products/options/set-rectangle":"<div class=\"form-field\" data-product-attribute=\"set-rectangle\"{{#if this.display_name '==' 'Subscribe for Auto-Ship'}}style=\"display:none;\"{{/if}}>\r\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\r\n        {{#if required}}\r\n            <small>{{lang 'common.required'}}</small>\r\n        {{/if}}\r\n        {{this.display_name}}:\r\n        <span data-option-value></span>\r\n    </label>\r\n\r\n    {{#each this.values}}\r\n        <input\r\n            class=\"form-radio {{#if ../display_name '==' 'Subscribe for Auto-Ship'}}{{this.label}}{{/if}}\"\r\n            type=\"radio\"\r\n            id=\"attribute_{{id}}\"\r\n            name=\"attribute[{{../id}}]\"\r\n            value=\"{{id}}\"\r\n            {{#if selected}}checked{{/if}}\r\n            {{#if ../required}}required{{/if}}>\r\n\r\n        <label class=\"form-option\" for=\"attribute_{{id}}\" data-product-attribute-value=\"{{id}}\">\r\n            <span class=\"form-option-variant\">{{this.label}}</span>\r\n        </label>\r\n    {{/each}}\r\n</div>\r\n"}
