{"components/common/paginator":"<div class=\"pagination\">    \r\n    <ul class=\"pagination-list\">\r\n        {{#if previous}}\r\n            <li class=\"pagination-item pagination-item--previous\">\r\n                <a class=\"pagination-link\" href=\"{{previous}}\" {{#unless reload}}data-faceted-search-facet{{/unless}}><svg><use xlink:href=\"#icon-arrow-right\"></use></svg></a>\r\n            </li>\r\n        {{/if}}\r\n        {{#each links}}\r\n            {{#if this.number '==' ../current}}\r\n                <li class=\"pagination-item pagination-item--current\">\r\n                    <a class=\"pagination-link\" href=\"{{this.url}}\" {{#unless reload}}data-faceted-search-facet{{/unless}}>{{this.number}}</a>\r\n                </li>\r\n            {{else}}\r\n                <li class=\"pagination-item\">\r\n                    <a class=\"pagination-link\" href=\"{{this.url}}\" {{#unless reload}}data-faceted-search-facet{{/unless}}>{{this.number}}</a>\r\n                </li>\r\n            {{/if}}\r\n        {{/each}}\r\n\r\n        {{#if next}}\r\n            <li class=\"pagination-item pagination-item--next\">\r\n                <a class=\"pagination-link\" href=\"{{next}}\" {{#unless reload}}data-faceted-search-facet{{/unless}}><svg><use xlink:href=\"#icon-arrow-right\"></use></svg></a>\r\n            </li>\r\n        {{/if}}\r\n    </ul>\r\n</div>\r\n","components/halothemes/brands-list":"<div>\n    <ul class=\"brandGrid\" data-brands-list data-brands-list-next=\"{{pagination.brands.next}}\" data-azbrands=\"azBrands\">\n        {{#each brands}}\n            <li class=\"brand\" data-brand-code=\"{{dashcase name}}\">\n                <article class=\"card {{#if alternate}}card--alternate{{/if}}\">\n                    <figure class=\"card-figure\">\n                        <a href=\"{{url}}\">\n                            <img class=\"card-image\" src=\"{{getImage image 'brand_size' (cdn ../theme_settings.default_image_brand)}}\" alt=\"{{name}}\" title=\"{{name}}\">\n                        </a>\n                    </figure>\n                    <div class=\"card-body\">\n                        <h4 class=\"card-title\">\n                            <a href=\"{{url}}\">{{name}}</a>\n                        </h4>\n                    </div>\n                </article>\n            </li>\n        {{/each}}\n    </ul>\n    {{> components/common/paginator pagination.brands}}\n</div>\n"}
