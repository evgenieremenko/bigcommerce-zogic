omponents/halothemes/halo-dropdown-login":"<div class=\"customer-dropdown-login hidden-xs hidden-sm\">\r\n    <div class=\"customer-login\">\r\n        {{#if customer}}\r\n        <a href=\"{{urls.auth.logout}}\"><span>{{lang 'common.logout'}}</span></a>\r\n        {{else}}\r\n        <a href=\"javascript:void(0);\" class=\"toggle-dropdown-login\">{{lang 'common.login'}}</a>\r\n        <div id=\"dropdown-login\">\r\n            <h3>Customer Login</h3>\r\n            <div class=\"pad-popup\">\r\n                <form class=\"login-popup\" id=\"loginForm\" action=\"{{settings.secure_base_url}}/login.php?action=check_login\" method=\"post\" name=\"mainLoginForm\">\r\n                    <fieldset class=\"form-fieldset\">\r\n                       <div class=\"form-field\">\r\n                            <label class=\"form-label\" for=\"login_email\">{{lang 'login.field_email'}}</label>\r\n                            <input class=\"form-input\" name=\"login_email\" id=\"login_email\" type=\"email\">\r\n                        </div>\r\n                        <div class=\"form-field\">\r\n                            <label class=\"form-label\" for=\"login_pass\">{{lang 'login.field_password'}}</label>\r\n                            <input class=\"form-input\" id=\"login_pass\" type=\"password\" name=\"login_pass\">\r\n                        </div>\r\n                        <div class=\"form-actions\">\r\n                            <input type=\"submit\" class=\"btn btn-alt btn-lg\" value=\"{{lang 'common.login'}}\">\r\n                            <a class=\"forgot-password\" href=\"{{urls.auth.forgot_password}}\">{{lang 'login.forgot_password' }}</a>\r\n                        </div>\r\n                    </fieldset>\r\n                </form>\r\n                <div class=\"separated-form\">\r\n                    <a href=\"{{urls.auth.create_account}}\" class=\"btn btn-primary\"><span>{{lang 'login.new_customer.create_a_new_account'}}</span></a>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        {{/if}}\r\n    </div>\r\n</div>\r\n","components/halothemes/header-phone-number":" <a href=\"tel:{{settings.phone_number}}\"><span>{{settings.phone_number}}</span></a> <span>or</span> <a href=\"/contact-us/\"><span>Contact Us</span></a>\n","components/halothemes/shipping-estimator":"{{#if shipping_cost}}\r\n    <div class=\"cart-total-value\">\r\n        <div class=\"subtotal shipping-estimate-show\">\r\n            <a href=\"#\" class=\"shipping-estimate-value\">{{shipping_cost.formatted}}</a>\r\n        </div>\r\n    </div>\r\n{{else}}\r\n    <div class=\"cart-total-value\">\r\n        <button class=\"shipping-estimate-show\"><span>{{lang 'cart.shipping_estimator.add_info'}}</span></button>\r\n        <button class=\"shipping-estimate-hide\" style=\"display: none;\"><span>Cancel</span></button>\r\n    </div>\r\n{{/if}}\r\n \r\n<div class=\"shipping-estimator u-hiddenVisually\" id=\"shipping-estimator-top\">\r\n    <form class=\"form estimator-form\" data-shipping-estimator>\r\n        <dl>\r\n            <dt class=\"estimator-form-label shipping-country\">\r\n                <label class=\"form-label\" for=\"shipping-country\">{{lang 'cart.shipping_estimator.select_a_country'}}</label>\r\n            </dt>\r\n\r\n            <dd class=\"estimator-form-input shipping-country\">\r\n                <select class=\"form-select\" id=\"shipping-country\" name=\"shipping-country\" data-field-type=\"Country\">\r\n                    <option>{{lang 'cart.shipping_estimator.select_a_country'}}</option>\r\n                    {{#each countries}}\r\n                        <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\r\n                            {{name}}\r\n                        </option>\r\n                    {{/each}}\r\n                </select>\r\n            </dd>\r\n\r\n            <dt class=\"estimator-form-label\">\r\n                <label class=\"form-label\" for=\"shipping-state\">{{lang 'cart.shipping_estimator.select_a_state'}}</label>\r\n            </dt>\r\n\r\n            <dd class=\"estimator-form-input shipping-state\">\r\n                {{#if states.length}}\r\n                    <select class=\"form-select\" id=\"shipping-state\" name=\"shipping-state\" data-field-type=\"State\">\r\n                        <option>{{lang 'cart.shipping_estimator.select_a_state'}}</option>\r\n                        {{#each states}}\r\n                            <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\r\n                                {{name}}\r\n                            </option>\r\n                        {{/each}}\r\n                    </select>\r\n                {{else}}\r\n                    <input class=\"form-input\" type=\"text\" id=\"shipping-state\" name=\"shipping-state\" data-field-type=\"State\" placeholder=\"{{lang 'cart.shipping_estimator.select_a_state'}}\">\r\n                {{/if}}\r\n            </dd>\r\n\r\n            <dt class=\"estimator-form-label shipping-city\">\r\n                <label class=\"form-label\" for=\"shipping-city\">{{lang 'cart.shipping_estimator.suburb_city'}}</label>\r\n            </dt>\r\n            <dd class=\"estimator-form-input shipping-city\">\r\n                <input class=\"form-input\" type=\"text\" id=\"shipping-city\" name=\"shipping-city\" value=\"{{selected_city}}\" placeholder=\"{{lang 'cart.shipping_estimator.suburb_city'}}\">\r\n            </dd>\r\n\r\n            <dt class=\"estimator-form-label\">\r\n                <label class=\"form-label\" for=\"shipping-zip\">{{lang 'cart.shipping_estimator.zip_postal_code'}}</label>\r\n            </dt>\r\n            <dd class=\"estimator-form-input shipping-zip\">\r\n                <input class=\"form-input\" type=\"text\" id=\"shipping-zip\" name=\"shipping-zip\" value=\"{{selected_zip}}\" placeholder=\"{{lang 'cart.shipping_estimator.zip_postal_code'}}\">\r\n            </dd>\r\n            <button class=\"btn btn-primary btn-lg shipping-estimate-submit\">{{lang 'cart.shipping_estimator.estimate_shipping'}}</button>\r\n        </dl>\r\n    </form>\r\n    <div class=\"shipping-quotes\"></div>\r\n</div>\r\n<script type=\"text/javascript\">\r\n    var url = location.pathname;\r\n    if(url == \"/cart.php\"){\r\n        var element = document.getElementById('shipping-estimator-top');\r\n        element.parentNode.removeChild(element);\r\n    }\r\n</script>\n"}
