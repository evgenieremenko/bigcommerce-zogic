{"components/common/subscription-form":"<div id=\"footerSubcribeForm\">\r\n    <h5><span style=\"font-size: 25px; font-weight: 700; color: #fff; font-family: Roboto Condensed\">Sign Up for Email</span></h5>\r\n    <p class=\"noteSubcribeForm\"><span style=\"font-size: 15px; font-weight: 400; line-height: 25px;\">Join thousands of fitness pros and get the latest deals, special offers, and resources right to your inbox.</span></p>\r\n    \r\n    <!--[if lte IE 8]>\r\n    <script charset=\"utf-8\" type=\"text/javascript\" src=\"//js.hsforms.net/forms/v2-legacy.js\"></script>\r\n    <![endif]-->\r\n    <script charset=\"utf-8\" type=\"text/javascript\" src=\"//js.hsforms.net/forms/v2.js\"></script>\r\n    <script>\r\n     hbspt.forms.create({\r\n    portalId: \"477652\",\r\n    formId: \"a1844ba4-01f1-4c55-929c-1c9696c4c4cf\"\r\n    });\r\n\r\n    var b = setInterval(function(){\r\n        // Form 0\r\n        for (let i = 0; i < $('iframe').length; i++) {\r\n            var iframe = document.getElementById(\"hs-form-iframe-\"+i);\r\n            if (iframe) {\r\n                var form0 = $(iframe.contentWindow.document)[0];\r\n                if ($(iframe).parents('#footerSubcribeForm').length == 1) {\r\n                $(form0).find('.hs_email').css({\"display\":\"inline-block\",\"width\":\"100%\",\"max-width\":\"500px\",\"margin-bottom\":\"0\"}).addClass('is_ready');\r\n                $(form0).find('.hs_email label').css({\"display\":\"none\"});\r\n                $(form0).find('.hs_email .hs-input').css({\"width\":\"100%\", \"max-width\":\"100%\",\"padding-right\":\"25px\",\"color\":\"#bababb\",\"font-style\":\"italic\",\"font-family\":\"Roboto,Arial,Helvetica,sans-serif\"});\r\n                \r\n                $(form0).find('.hs_submit').css({\"display\":\"inline-block\", \"position\":\"absolute\", \"top\":\"0px\", \"right\":\"0px\"});\r\n                $(form0).find('.hs_submit .actions').css({\"padding\":\"0\", \"margin\":\"0\"});\r\n                $(form0).find('.hs_submit .actions .hs-button').css({\"padding\":\"14px 24px 12px 24px\",\"border-top-left-radius\":\"0\",\"border-bottom-left-radius\":\"0\",\"text-transform\":\"uppercase\",\"background\":\"#008bab\",\"border\":\"1px solid #008bab\",\"font-family\":\"Roboto,Arial,Helvetica,sans-serif\"});\r\n            }\r\n                if($(form0).find('.hs_email').length && $(form0).find('.hs_email').hasClass('is_ready')){\r\n                    clearInterval(b);\r\n                }\r\n            }\r\n        }\r\n\r\n    }, 500);\r\n    </script>\r\n</div>\r\n"}
