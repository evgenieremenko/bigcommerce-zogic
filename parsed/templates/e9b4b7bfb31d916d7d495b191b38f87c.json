{"components/faceted-search/selected-facets":"<div id=\"siderefineFilters\" class=\"sidebarBlock accordion-block sidebarEffects-window\">\r\n   <div\r\n        class=\"accordion-navigation toggleLink\"\r\n        role=\"button\"\r\n        data-collapsible=\"#facetedSearch-content--refineFilters\">\r\n        <h5 class=\"accordion-title side-module-heading\"><span>{{lang 'search.faceted.selected.title'}}</span></h5>\r\n\r\n        <div class=\"accordion-navigation-actions\">\r\n            {{#if selected}}\r\n                <a href=\"{{ remove_url }}\" class=\"facetedSearch-clearLink\" data-faceted-search-facet>{{lang 'search.faceted.clear'}}</a>\r\n            {{/if}}\r\n            <i class=\"fa fa-chevron-up accordion-indicator toggleLink-text toggleLink-text--off\">\r\n            </i>\r\n            <i class=\"fa fa-chevron-down accordion-indicator toggleLink-text toggleLink-text--on\">\r\n            </i>\r\n        </div>\r\n    </div>\r\n    <div id=\"facetedSearch-content--refineFilters\" class=\"accordion-content\">\r\n        {{#if this.items}}\r\n            <ul class=\"inlineList inlineList--labels\">\r\n                {{#each this.items}}\r\n                    <li>\r\n                        <a\r\n                            href=\"{{ url }}\"\r\n                            class=\"facetLabel\"\r\n                            rel=\"nofollow\"\r\n                            data-faceted-search-facet>\r\n                            {{#if facet '===' 'rating'}}\r\n                                {{lang 'search.faceted.selected.rating-label' rating=title}}\r\n                            {{else}}\r\n                                {{ title }}\r\n                            {{/if}}\r\n\r\n                            <svg class=\"icon\">\r\n                                <use xlink:href=\"#icon-close\" />\r\n                            </svg>\r\n                        </a>\r\n                    </li>\r\n                {{/each}}\r\n            </ul>\r\n        {{else}}\r\n            <div class=\"alert alert-warning text-center\">{{lang 'search.faceted.selected.no-filters'}}</div>\r\n        {{/if}}\r\n\r\n        {{#if remove_all_url}}\r\n            <a class=\"btn btn-alt btn-block\" href=\"{{ remove_all_url }}\" data-faceted-search-facet>\r\n                {{lang 'search.faceted.selected.clear-all'}}\r\n            </a>\r\n        {{/if}}\r\n    </div>\r\n</div>\r\n\r\n"}
