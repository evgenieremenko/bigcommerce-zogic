n                        <div class=\"form-field\">\r\n                            <label class=\"form-label\" for=\"login_pass\">{{lang 'login.field_password'}}</label>\r\n                            <input class=\"form-input\" id=\"login_pass\" type=\"password\" name=\"login_pass\">\r\n                        </div>\r\n                        <div class=\"form-actions\">\r\n                            <input type=\"submit\" class=\"btn btn-alt btn-lg\" value=\"{{lang 'common.login'}}\">\r\n                            <a class=\"forgot-password\" href=\"{{urls.auth.forgot_password}}\">{{lang 'login.forgot_password' }}</a>\r\n                        </div>\r\n                    </fieldset>\r\n                </form>\r\n                <div class=\"separated-form\">\r\n                    <a href=\"{{urls.auth.create_account}}\" class=\"btn btn-primary\"><span>{{lang 'login.new_customer.create_a_new_account'}}</span></a>\r\n                </div>\r\n            </div>\r\n        </div>\r\n        {{/if}}\r\n    </div>\r\n</div>\r\n"}
