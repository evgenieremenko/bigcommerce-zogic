{"components/halothemes/halo-request-a-quote":"<!-- BEGIN ASK AN EXPERT MODAL -->\r\n<div id=\"halo-request-a-quote-form\" class=\"modal\" data-reveal>\r\n   <a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'common.close'}}\" role=\"button\">\r\n      <span aria-hidden=\"true\">&#215;</span>\r\n   </a>\r\n   <div class=\"modal-content\">\r\n      <div class=\"modal-header\">\r\n         <h2 class=\"modal-header-title\">Request a Quote</h2>\r\n      </div>\r\n      <div class=\"modal-body\">\r\n         <div id=\"contact_results\"></div>\r\n         <div class=\"form-inner\">\r\n                           <div class=\"form-raq\">\r\n                              <!--[if lte IE 8]>\r\n                              <script charset=\"utf-8\" type=\"text/javascript\" src=\"//js.hsforms.net/forms/v2-legacy.js\"></script>\r\n                              <![endif]-->\r\n                              <script charset=\"utf-8\" type=\"text/javascript\" src=\"https://js.hsforms.net/forms/v2.js\"></script>\r\n                              <script type=\"text/javascript\">// <![CDATA[\r\n                              hbspt.forms.create({ \r\n                                portalId: '477652',\r\n                                formId: 'f6a6cd35-05a1-4f22-89f9-2415d2aecde6',\r\n                                target: '.form-raq'\r\n                              });\r\n                              // ]]></script>\r\n                            </div>\r\n            \r\n         </div>\r\n      </div>\r\n   </div>\r\n</div>\r\n<!-- END ASK AN EXPERT MODAL -->\r\n\r\n<script>\r\n   $(document).ready(function() {\r\n     $('.prod-raq,.raq-btn-close').on('click', function(event){\r\n         event.preventDefault();\r\n         var raq = $('body');\r\n         if(! raq.hasClass(\"form-raq-show\")){\r\n           raq.addClass(\"form-raq-show\");\r\n         } else {\r\n           raq.removeClass(\"form-raq-show\");\r\n         }\r\n     });\r\n   });\r\n </script>\r\n"}
