{"components/category/side-shop-by-brand":"<div class=\"accordion-block sidebarBlock sidebarEffects-window\" id=\"sideShopByBrands\">\r\n    <div\r\n        class=\"accordion-navigation toggleLink\"\r\n        role=\"button\"\r\n        data-collapsible=\"#facetedSearch-content--ShopByBrands\">\r\n        <h5 class=\"accordion-title side-module-heading\"><span>{{lang 'brand.label'}}</span></h5>\r\n\r\n        <div class=\"accordion-navigation-actions\">\r\n            {{#if selected}}\r\n                <a href=\"{{ remove_url }}\" class=\"facetedSearch-clearLink\" data-faceted-search-facet>{{lang 'search.faceted.clear'}}</a>\r\n            {{/if}}\r\n            <i class=\"fa fa-chevron-up accordion-indicator toggleLink-text toggleLink-text--off\">\r\n            </i>\r\n            <i class=\"fa fa-chevron-down accordion-indicator toggleLink-text toggleLink-text--on\">\r\n            </i>\r\n        </div>\r\n    </div>\r\n    <div id=\"facetedSearch-content--ShopByBrands\" class=\"navList accordion-content\">\r\n        <ul class=\"navList-scroll\">\r\n            {{#each shop_by_brand}}\r\n                <li>\r\n                    <a href=\"{{url}}\">{{name}}</a>\r\n                </li>\r\n            {{/each}}\r\n        </ul>\r\n        <a class=\"view-all-brands\" href=\"{{urls.brands}}\"><i class=\"fa fa-caret-right\"></i>{{lang 'common.view_all'}}</a>\r\n    </div>\r\n</div>\r\n"}
